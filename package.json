    "next": "^14.2.0",
    "next-auth": "^4.24.0",
    "node-schedule": "^2.1.1",
    "react": "^18.3.0",
    "react-dom": "^18.3.0",
    "recharts": "^3.3.0",
    "typescript": "^5.4.0",
    "zod": "^3.22.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.4.0",
    "@testing-library/react": "^14.2.0",
    "@testing-library/user-event": "^14.5.0",
    "@types/bcrypt": "^5.0.2",
    "@types/jest": "^29.5.0",
    "@types/node": "^20.11.0",
    "@types/node-schedule": "^2.1.8",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
